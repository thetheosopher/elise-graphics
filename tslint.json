{
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "rules": {
        "no-console": false,
        "variable-name": {
            "options": [
              "allow-leading-underscore",
              "allow-pascal-case"
            ]
          },
        "no-bitwise": false,
        "no-empty": false
    }
}